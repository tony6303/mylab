	<script>
		var data = {
			"username" : "lee",
			"password" : "1234",
			"phone" : "0101233"
		}
		$.ajax({
			type : "post",
			url : "localhost:8080/user",
			data : JSON.stringify(data),
			contentType : "application/json; charset=utf-8",
			dataType : "json"
		}).done(function(result) {
			console.log(result);
		})
	</script>
